{
    "name": "JadoLuz Theme",
    "$schema": "vscode://schemas/color-theme",
    "type": "light",
    "colors": {
        "activityBar.background": "#F0F0F0",
        "activityBar.border": "#00000018",
        "activityBar.foreground": "#000000",
        "activityBarBadge.background": "#BD1B0F",
        "dropdown.background": "#F8F8F8",
        "dropdown.border": "#00000018",
        "editor.background": "#FFFFFF",
        "editor.foreground": "#000000",
        "editorBracketMatch.background": "#0064001A",
        "editorBracketMatch.border": "#9fA3A0",
        "editor.findMatchBackground": "#309C5AB9",
        "editor.findMatchHighlightBackground": "#EA5C0055",
        "editorGroupHeader.tabsBackground": "#F0F0F0",
        "editorGroupHeader.tabsBorder": "#00000018",
        "editorHint.foreground": "#5E5959",
        "editorIndentGuide.activeBackground": "#00000018",
        "editorIndentGuide.background": "#00000010",
        "editorLightBulb.foreground": "#dd76007a",
        "editorLightBulbAutoFix.foreground": "#1B40B9B7",
        "editorLineNumber.activeForeground": "#000000",
        "editorLineNumber.foreground": "#999999",
        "editorUnnecessaryCode.opacity": "#00000080",
        "editorWarning.foreground": "#E9C60091",
        "editor.wordHighlightBackground": "#3A4D8340",
        "editor.wordHighlightStrongBackground": "#08268840",
        "errorForeground": "#A10D19C5",
        "extensionBadge.remoteBackground": "#E00C0C",
        "extensionBadge.remoteForeground": "#ffffff",
        "extensionButton.prominentBackground": "#349B3A",
        "extensionButton.prominentForeground": "#ffffff",
        "extensionButton.prominentHoverBackground": "#227025",
        "focusBorder": "#00cc777a",
        "gitDecoration.addedResourceForeground": "#0c7c3bee",
        "gitDecoration.conflictingResourceForeground": "#8b6cc4",
        "gitDecoration.deletedResourceForeground": "#ad3b07",
        "gitDecoration.ignoredResourceForeground": "#8f8f91",
        "gitDecoration.modifiedResourceForeground": "#2D76D4",
        "gitDecoration.submoduleResourceForeground": "#8923c4",
        "gitDecoration.untrackedResourceForeground": "#3c6b3c",
        "icon.foreground": "#474646",
        "input.background": "#F8F8F8",
        "input.border": "#00000018",
        "list.activeSelectionBackground": "#00000018",
        "list.activeSelectionForeground": "#000000",
        "list.hoverBackground": "#00000008",
        "list.inactiveSelectionBackground": "#00000010",
        "notificationLink.foreground": "#E00C0C",
        "sideBar.background": "#F8F8F8",
        "sideBar.border": "#00000018",
        "sideBarSectionHeader.background": "#00000010",
        "statusBar.background": "#F0F0F0",
        "statusBar.border": "#00000018",
        "statusBar.debuggingBackground": "#BD1B0F",
        "statusBar.debuggingForeground": "#FFFFFF",
        "statusBar.foreground": "#000000",
        "statusBarItem.activeBackground": "#ffffff2e",
        "statusBarItem.hoverBackground": "#ffffff1f",
        "statusBarItem.prominentBackground": "#00000080",
        "statusBarItem.prominentForeground": "#000000",
        "statusBarItem.prominentHoverBackground": "#0000004d",
        "tab.activeForeground": "#333333",
        "tab.activeModifiedBorder": "#2D76D4",
        "tab.inactiveForeground": "#333333b3",
        "tab.inactiveModifiedBorder": "#2d75d4b7",
        "tab.unfocusedActiveBackground": "#F8F8F8",
        "tab.unfocusedActiveBorder": "#ee00009c",
        "tab.unfocusedActiveForeground": "#333333b3",
        "tab.unfocusedActiveModifiedBorder": "#2d75d4b7",
        "tab.unfocusedInactiveForeground": "#33333359",
        "tab.unfocusedInactiveModifiedBorder": "#2d75d483",
        "tab.activeBackground": "#F8F8F8",
        "tab.activeBorder": "#BD1B0F",
        "tab.border": "#00000018",
        "tab.inactiveBackground": "#E0E0E0",
        "terminal.border": "#808080b9",
        "terminal.selectionBackground": "#00000060",
        "textLink.activeForeground": "#E00C0C",
        "textLink.foreground": "#BD1B0F",
        "textBlockQuote.background": "#7f7f7f1a",
        "textBlockQuote.border": "#0300cc80",
        "textCodeBlock.background": "#dcdcdc66",
        "textPreformat.foreground": "#bd3118ef",
        "textSeparator.foreground": "#0000002e",
        "titleBar.activeBackground": "#dddddd",
        "titleBar.activeForeground": "#333333",
        "titleBar.inactiveBackground": "#dddddd99",
        "titleBar.inactiveForeground": "#33333399",
        "tree.indentGuidesStroke": "#9e9d9d",
        "panel.border": "#00000018",
        "statusBar.noFolderBackground": "#706E6E",
        "statusBar.noFolderBorder": "#00000010",
        "statusBar.noFolderForeground": "#FFFFFF",
        "statusBarItem.remoteBackground": "#E00C0C",
        "statusBarItem.remoteForeground": "#FFFFFF",

    },
    "tokenColors": [{
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "beginning.punctuation.definition.quote.markdown",
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "scope": "beginning.punctuation.definition.list.markdown",
            "settings": {
                "foreground": "#009059"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#E24E13"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "name": "Function declarations",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars"
            ],
            "settings": {
                "foreground": "#9315DB"
            }
        },
        {
            "name": "Types declaration and references",
            "scope": [
                "meta.return-type",
                "support.class",
                "support.type",
                "entity.name.type",
                "entity.name.class",
                "storage.type.numeric.go",
                "storage.type.byte.go",
                "storage.type.boolean.go",
                "storage.type.string.go",
                "storage.type.uintptr.go",
                "storage.type.error.go",
                "storage.type.rune.go",
                "storage.type.cs",
                "storage.type.generic.cs",
                "storage.type.modifier.cs",
                "storage.type.variable.cs",
                "storage.type.annotation.java",
                "storage.type.generic.java",
                "storage.type.java",
                "storage.type.object.array.java",
                "storage.type.primitive.array.java",
                "storage.type.primitive.java",
                "storage.type.token.java",
                "storage.type.groovy",
                "storage.type.annotation.groovy",
                "storage.type.parameters.groovy",
                "storage.type.generic.groovy",
                "storage.type.object.array.groovy",
                "storage.type.primitive.array.groovy",
                "storage.type.primitive.groovy"
            ],
            "settings": {
                "foreground": "#009059"
            }
        },
        {
            "name": "Types declaration and references, TS grammar specific",
            "scope": [
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#009059"
            }
        },
        {
            "name": "Control flow keywords",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "name": "Variable and parameter name",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "name": "Object keys, TS grammar specific",
            "scope": [
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "name": "CSS property value",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "name": "Regular expression groups",
            "scope": [
                "punctuation.definition.group.regexp",
                "punctuation.definition.group.assertion.regexp",
                "punctuation.definition.character-class.regexp",
                "punctuation.character.set.begin.regexp",
                "punctuation.character.set.end.regexp",
                "keyword.operator.negation.regexp",
                "support.other.parenthesis.regexp"
            ],
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": [
                "constant.character.character-class.regexp",
                "constant.other.character-class.set.regexp",
                "constant.other.character-class.regexp",
                "constant.character.set.regexp"
            ],
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": [
                "keyword.operator.or.regexp",
                "keyword.control.anchor.regexp"
            ],
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "keyword.operator.quantifier.regexp",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "constant.character",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#4700CC"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#009059"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#BD1B0F"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#E00C0C"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#8026CA"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "entity.name.variable",
            "settings": {
                "foreground": "#000000"
            }
        }
    ]
}